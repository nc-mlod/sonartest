pipeline {
    agent { label 'Built-In Node' }
    options {
        buildDiscarder(logRotator(numToKeepStr: '5'))
    }

    stages {
        stage('Scan') {
            steps {
                withSonarQubeEnv(installationName: 'sq1') {
                    sh '.mvnw clean org.sonarsource.scanner.maven:sonar-maven-plugin:sonar'
                }
            }
        }
    }
}